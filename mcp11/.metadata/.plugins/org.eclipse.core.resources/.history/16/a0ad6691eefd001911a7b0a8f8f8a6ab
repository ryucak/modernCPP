#include <iostream>
#include <string>

class vignere{
	std::string key;
public:
	vignere(std::string key){
		key = key;
	};
};


std::string vigenere_encrypt(std::string clr, std::string key)
{
	std::string cph = "";

	for (int i = 0; i < clr.length(); ++i) {
		char c = clr.c_str()[i];
		if(isalpha(c) && isupper(c)){
			c += (key.c_str()[i%key.length()] - "A");
		}
	}

	return cph;
}

std::string vigenere_decrypt(std::string cph, std::string key)
{
	std::string clr = "";

	for (int i = 0; i < clr.length(); ++i) {
		char c = clr.c_str()[i];
		if(isalpha(c) && isupper(c)){
			c -= key.c_str()[i%key.length()];
		}
	}

	return clr;
}


int main(int argc, char **argv) {
	std::string hoge;

	std::cin >> hoge;

	std::string hoge_crypt = vigenere_encrypt(hoge,"ARM");
	std::cout <<  << std::endl;

	return 0;
}


