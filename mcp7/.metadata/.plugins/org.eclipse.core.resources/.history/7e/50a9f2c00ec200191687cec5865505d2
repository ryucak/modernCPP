#include <mutex>
#include <string>
#include <iostream>
#include <thread>
#include <vector>

class Logger {
	std::mutex m;

public:
	Logger() { m = new std::mutex(); }

	void log(std::string msg) {
		m.lock();
		std::cout << msg << std::endl;
		m.unlock();
	}
};


Logger log;

int main(int argc, char **argv) {
	std::vector<std::thread> thr;

	for (int id = 0; id < 100; ++id) {

	}
}

