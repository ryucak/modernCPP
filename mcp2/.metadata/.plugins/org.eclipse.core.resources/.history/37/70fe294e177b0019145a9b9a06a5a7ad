#ifndef TEMPERATURE_H_
#define TEMPERATURE_H_

#include <string>

class temperature {
	uint32_t abs;
public:
	temperature(){abs = 0;}

	std::string toKelvin(){
		return abs;
	}

	virtual ~temperature(){}
};

#endif /* TEMPERATURE_H_ */
