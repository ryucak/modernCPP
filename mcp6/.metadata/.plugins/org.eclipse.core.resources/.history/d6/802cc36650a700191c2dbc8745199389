#include <vector>
#include <string>

std::vector<std::string> comv_phone_numbers(
		std::vector<std::string> const & numbers,
		std::string const & countryCode) {
	std::vector<std::string> result;

	for(auto it:numbers){
		std::string comv_str = "";

		if(it.find('+' + countryCode) == 0){
			comv_str = it;
		}

		if(it.find(countryCode) || ){
			result.push_back(it);
		}
	}
	return result;
}




