#include <set>

class Cell{
	bool alive;
	std::set<Cell>	neighbors;
public:
	int count();
	void next(bool alive) {this->alive = alive;}
};

int Cell::count(void){
	int cnt;
	for (Cell c :neighbors) {
		if(c.alive){
			cnt++;
		}
	}
	return cnt;
}

