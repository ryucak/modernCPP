#include <set>
#include <vector>

class Cell{
	bool alive;
	std::set<Cell>	neighbors;
public:
	int count();
	void next_gen(bool alive) {this->alive = alive;}
};

int Cell::count(void){
	int cnt;
	for (Cell c :neighbors) {
		if(c.alive){
			cnt++;
		}
	}
	return cnt;
}

#define WIDTH 50
#define HEIGHT 50

bool judge(int cnt, bool alive){
	switch(cnt){
	case 2:
		if(alive) return true;
		break;
	case 3:
		return true;
	}
	return false;
}

