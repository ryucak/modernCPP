#include <String>
#include <cstdlib>
#include <png++/png.hpp>

using namespace png;

int main(int argc, char **argv) {

	if(argc < 4) return -1;

	int width = std::atoi(argv[1]);
	int hehght = std::atoi(argv[2]);
	std::string filename = argv[3];

	image<rgb_pixel> img("imput.png");

	img.resize(width,hehght);

	img.write(filename);

	return 0;
}



