#include <iostream>
#include <chrono>

using namespace std::chrono;

int durationDays(system_clock::time_point a, system_clock::time_point b){
	return duration_cast<hours>(a-b).count()/24;
}


int main(int argc, char **argv) {
	std::cout << durationDays() << std::endl;;
}
