#include <iostream>
#include <chrono>

using namespace std::chrono;

enum Scale{
	microseconds,
	milliseconds,
	seconds,
};

template <typename Func, typename... Args> int elapse(Func target,Scale scale, Args ... args){
	auto start = system_clock::now();
	target(args);
	auto end = system_clock::now();
}

