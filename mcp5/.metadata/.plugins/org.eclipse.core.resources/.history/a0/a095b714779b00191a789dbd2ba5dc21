#include <iostream>
#include <chrono>

using namespace std::chrono;

template <typename Func,typename Duration, typename... Args> Duration elapse(Func target,Args ... args){
	auto start = system_clock::now();
	target(args);
	return duration_cast<Duration>(system_clock::now() - start);
}

